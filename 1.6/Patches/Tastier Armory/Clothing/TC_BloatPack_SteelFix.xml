<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Tastier Armory - Medieval European Armors</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
			
			
				<li Class="PatchOperationConditional">
					<xpath>Defs/ThingDef[defName="TC_Scabbard"]/costList/Steel</xpath>
					<match Class="PatchOperationSetName">
						<xpath>Defs/ThingDef[defName="TC_Scabbard"]/costList/Steel</xpath>
						<name>DankPyon_IronIngot</name>
					</match>
				</li>
				
				<li Class="PatchOperationConditional">
					<xpath>Defs/ThingDef[defName="TC_HeaterShield"]/costList/Steel</xpath>
					<match Class="PatchOperationSetName">
						<xpath>Defs/ThingDef[defName="TC_HeaterShield"]/costList/Steel</xpath>
						<name>DankPyon_IronIngot</name>
					</match>
				</li>
				
				<li Class="PatchOperationConditional">
					<xpath>Defs/ThingDef[defName="TC_KiteShield"]/costList/Steel</xpath>
					<match Class="PatchOperationSetName">
						<xpath>Defs/ThingDef[defName="TC_KiteShield"]/costList/Steel</xpath>
						<name>DankPyon_IronIngot</name>
					</match>
				</li>
				
				<li Class="PatchOperationConditional">
					<xpath>Defs/ThingDef[defName="TC_RoundShield"]/costList/Steel</xpath>
					<match Class="PatchOperationSetName">
						<xpath>Defs/ThingDef[defName="TC_RoundShield"]/costList/Steel</xpath>
						<name>DankPyon_IronIngot</name>
					</match>
				</li>
			
			</operations>
		</match>
		
	</Operation>
	
</Patch>