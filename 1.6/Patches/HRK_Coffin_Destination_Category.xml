<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>[HRK] Medieval deathrest coffin</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="DeathrestBasicCoffin" or defName="DeathrestCoffin"]/designationCategory</xpath>
					<value>
						<designationCategory>Biotech</designationCategory>
					</value>
				</li>

			</operations>
		</match>
	</Operation>

</Patch>