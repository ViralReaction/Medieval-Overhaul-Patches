<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Medieval Genepacks - Alpha Genes Patch</li>
		</mods>
		<match Class="PatchOperationFindMod">
			<mods>
				<li>Medieval Biotech</li>
			</mods>
			<match Class="PatchOperationSequence">
				<operations>
				
					<li Class="PatchOperationReplace">
						<xpath>Defs/ThingDef[defName="AG_Alphapack"]/comps/li[@Class="ConsumableGenepack.CompProperties_ConsumableGenepack"]</xpath>
						<value>
							<li Class="MedievalBiotech.CompProperties_ConsumableGenepack"/>
						</value>
					</li>
					
					<li Class="PatchOperationReplace">
						<xpath>Defs/ThingDef[defName="AG_Mixedpack"]/comps/li[@Class="ConsumableGenepack.CompProperties_ConsumableGenepack"]</xpath>
						<value>
							<li Class="MedievalBiotech.CompProperties_ConsumableGenepack"/>
						</value>
					</li>

				</operations>
			</match>
		</match>
	</Operation>

</Patch>