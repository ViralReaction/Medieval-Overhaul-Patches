<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Medieval Biotech</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="Deathrest"]/prerequisites</xpath>
					<value>
						<prerequisites>
							<li>Esoteric_Studies</li>
						</prerequisites>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="Deathrest"]/researchViewX</xpath>
					<value>
						<researchViewX>14.00</researchViewX>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="Deathrest"]/researchViewY</xpath>
					<value>
						<researchViewY>5.05</researchViewY>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="Deathrest"]/techLevel</xpath>
					<value>
						<techLevel>Medieval</techLevel>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/ResearchProjectDef[defName="Deathrest"]</xpath>
					<value>
						<tab>DankPyon_MedievalResearchTab</tab>
					</value>
				</li>

				
			</operations>
		</match>
	</Operation>
</Patch>
