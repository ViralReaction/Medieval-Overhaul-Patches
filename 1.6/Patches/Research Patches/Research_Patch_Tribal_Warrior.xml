<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Tribal Warrior Set!</li>
		</mods>
		
		<match Class="PatchOperationSequence">
			<operations>
			
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="Gunmar_War_Bow"]/recipeMaker/researchPrerequisite</xpath>
					<value>
						<researchPrerequisite>Greatbow</researchPrerequisite>
					</value>
				</li>
				
				<li Class="PatchOperationRemove">
					<xpath>Defs/ResearchProjectDef[defName="Gunmar_WarBow"]</xpath>
				</li>
			
			</operations>
		</match>
		
	</Operation>
	
</Patch>