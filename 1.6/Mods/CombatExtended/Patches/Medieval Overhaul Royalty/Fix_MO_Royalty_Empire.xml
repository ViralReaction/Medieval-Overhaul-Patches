<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Medieval Overhaul: Royalty</li>
		</mods>
		<match Class="PatchOperationFindMod">
			<mods>
				<li>Vanilla Factions Expanded - Empire</li>
			</mods>
			<match Class="PatchOperationFindMod">
				<mods>
					<li>Combat Extended</li>
				</mods>

				<match Class="PatchOperationSequence">
					<operations>
			
						<li Class="PatchOperationReplace">
							<xpath>Defs/RoyalTitlePermitDef[defName="VFEI_HEShellsDrop"]/royalAid/itemsToDrop</xpath>
							<value>
								<itemsToDrop>
									<Pila>20</Pila>
								</itemsToDrop>
							</value>
						</li>
				
					</operations>
				</match>
			</match>
		</match>
	</Operation>
	
</Patch>