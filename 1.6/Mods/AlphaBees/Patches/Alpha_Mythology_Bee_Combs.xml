<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Mythology</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="RB_LightMagical_Honeycomb"]/butcherProducts</xpath>
					<value>
						<butcherProducts>
							<RB_PsychotropicHoney>7</RB_PsychotropicHoney>
							<RB_Beeswax>5</RB_Beeswax>
						</butcherProducts>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="RB_HeavyMagical_Honeycomb"]/butcherProducts</xpath>
					<value>
						<butcherProducts>
							<RB_PsychotropicHoney>10</RB_PsychotropicHoney>
							<RB_Beeswax>5</RB_Beeswax>
						</butcherProducts>
					</value>
				</li>

			</operations>
		</match>
	</Operation>

</Patch>