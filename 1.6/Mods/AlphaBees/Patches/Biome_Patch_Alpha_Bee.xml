<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<!-- Medieval Overhaul Biomes -->
	<Operation Class="PatchOperationConditional">
		<xpath>/Defs/BiomeDef[defName="DankPyon_DarkForest"]/wildPlants/RB_Plant_Hive_Temperate</xpath>
		<nomatch Class="PatchOperationAdd">
			<xpath>/Defs/BiomeDef[defName="DankPyon_DarkForest"]/wildPlants</xpath>
			<value>
				<RB_Plant_Hive_Temperate>0.01</RB_Plant_Hive_Temperate>
			</value>
		</nomatch>
	</Operation>
	
	<Operation Class="PatchOperationConditional">
		<xpath>/Defs/BiomeDef[defName="DankPyon_DarkForest"]/wildPlants/RB_Plant_Hive_Mild</xpath>
		<nomatch Class="PatchOperationAdd">
			<xpath>/Defs/BiomeDef[defName="DankPyon_DarkForest"]/wildPlants</xpath>
			<value>
				<RB_Plant_Hive_Mild>0.01</RB_Plant_Hive_Mild>
			</value>
		</nomatch>
	</Operation>

</Patch>