	<!-- Dried Human Meats -->
	<ThingDef ParentName="MealBase">
		<defName>DankPyon_DriedMeat_Human</defName>
		<label>Dried Human Meat</label>
		<description>Dried human meat to preserve and prolong shelf life. Does not rehydrate very well and only true cannibals can stomach it.</description>
		<graphicData>
			<texPath>Things/Item/Meal/DriedMeat</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<!--thingCategories>
			<li>DankPyon_DriedFood</li>
		</thingCategories-->
		<statBases>
			<MarketValue>13</MarketValue>
			<Nutrition>0.7</Nutrition>
		</statBases>
		<ingredient>
			<mergeCompatibilityTags>
			</mergeCompatibilityTags>
		</ingredient>
		<ingestible>
			<foodType>Meat</foodType>
			<preferability>DesperateOnly</preferability>
			<ingestEffect>EatMeat</ingestEffect>
			<ingestSound>RawMeat_Eat</ingestSound>
			<tasteThought>DankPyon_DriedMeat</tasteThought>
			<optimalityOffsetHumanlikes>-5</optimalityOffsetHumanlikes>
			<optimalityOffsetFeedingAnimals>-10</optimalityOffsetFeedingAnimals>
		</ingestible>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>60</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<rotatable>false</rotatable>
	</ThingDef>

